{
  "atomCode": "RepoCorrelator",
  "execution": {
    "packagePath": "RepoCorrelator-jar-with-dependencies.jar",
    "language": "java",
    "minimumVersion": "1.8",
    "demands": [],
    "target": "java -jar RepoCorrelator-jar-with-dependencies.jar"
  },
  "input": {
    "featureOption": {
      "label": "群组结构选项",
      "default": 0,
      "type": "enum-input",
      "list": [
        {
          "value": 0,
          "label": "默认方式",
          "desc": "项目群组迁移至目标GitLab一级群组下"
        },
        {
          "value": 1,
          "label": "自定义根群组",
          "desc": "项目群组迁移至目标GitLab指定群组下"
        }
      ],
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "rootId": {
      "label": "根群组Id",
      "default": "",
      "placeholder": "",
      "type": "vuex-input",
      "desc": "代码库是否迁移到指定群组下，如是，请正确输入；不输入按默认方式建立群组结构",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "featureOption",
            "value": 1
          }
        ]
      }
    },
    "projectCnName": {
      "label": "项目中文名",
      "default": "",
      "placeholder": "",
      "type": "vuex-input",
      "desc": "请输入项目中文名",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "projectEnName": {
      "label": "项目英文名",
      "default": "",
      "placeholder": "",
      "type": "vuex-input",
      "desc": "切勿输错项目英文名！！！",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "backend": {
      "label": "后端代码库地址列表",
      "default": "",
      "placeholder": "",
      "type": "vuex-textarea",
      "desc": "地址之间用回车分隔",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "ui": {
      "label": "前端代码库地址列表",
      "default": "",
      "placeholder": "",
      "type": "vuex-textarea",
      "desc": "地址之间用回车分隔",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "test": {
      "label": "测试脚本代码库地址列表",
      "default": "",
      "placeholder": "",
      "type": "vuex-textarea",
      "desc": "地址之间用回车分隔",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    }
  },
  "output": {
  }
}
