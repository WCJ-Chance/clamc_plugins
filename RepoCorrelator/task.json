{
  "atomCode": "RepoCorrelator",
  "execution": {
    "packagePath": "RepoCorrelator-jar-with-dependencies.jar",
    "language": "java",
    "minimumVersion": "1.8",
    "demands": [],
    "target": "java -jar RepoCorrelator-jar-with-dependencies.jar"
  },
  "input": {
    "featureOption": {
      "label": "迁移类型",
      "default": 0,
      "type": "enum-input",
      "list": [
        {
          "value": 0,
          "label": "同步分组及子项目",
          "desc": "同步分组及子项目"
        },
        {
          "value": 1,
          "label": "同步子项目",
          "desc": "项目群组迁移至目标GitLab指定群组下"
        },
        {
          "value": 2,
          "label": "同步配置文件",
          "desc": "同步配置文件"
        }
      ],
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "sourceGroupUrl": {
      "label": "源分组url",
      "default": "",
      "placeholder": "e.g. http://git.clamc.com/Climb/Business/ITOM",
      "type": "vuex-input",
      "desc": "源分组url",
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "featureOption",
            "value": 0
          }
        ]
      },
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "rootId": {
      "label": "目标父分组Id",
      "default": "",
      "placeholder": "",
      "type": "vuex-input",
      "desc": "目标分组的父分组Id, 设为顶层分组 填0",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "OR",
        "expression": [
          {
            "key": "featureOption",
            "value": 0
          },
          {
            "key": "featureOption",
            "value": 1
          }
        ]
      }
    },
    "repoUrlList": {
      "label": "某一个分组下的地址列表",
      "default": "",
      "placeholder": "",
      "type": "vuex-textarea",
      "desc": "用于部分代码库更新迁移",
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "featureOption",
            "value": 1
          }
        ]
      },
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "ifSetAllConfig": {
      "label": "",
      "default": false,
      "placeholder": "",
      "type": "atom-checkbox",
      "text": "是否全量迁移配置文件",
      "desc": "全量覆盖，谨慎操作",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "featureOption",
            "value": 2
          }
        ]
      }
    },
    "configUrlList": {
      "label": "要迁移的部分配置文件地址",
      "default": "",
      "placeholder": "",
      "type": "vuex-textarea",
      "desc": "用于部分代码库更新迁移",
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "featureOption",
            "value": 2
          },
          {
            "key": "ifSetAllConfig",
            "value": false
          }
        ]
      },
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    }
  },
  "output": {
  }
}
